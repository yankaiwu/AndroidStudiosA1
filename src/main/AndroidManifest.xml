<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppingapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppInitializeActivity"></activity>
        <activity android:name=".AdminActivity" />
        <activity android:name=".EmployeeActivity" />
        <activity android:name=".CustomerShopActivity" />
        <activity android:name=".CustomerCartActivity" />
        <activity android:name=".CustomerActivity" />
        <activity android:name=".CreateUserPopUp"
            android:theme="@style/AppTheme.PopUpTheme"></activity>
        <activity android:name=".CreateEmployeePopUp"
            android:theme="@style/AppTheme.PopUpTheme"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>